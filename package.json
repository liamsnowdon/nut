  "dependencies": {
    "@nuxtjs/seo": "^2.1.1",
    "@pinia/nuxt": "^0.9.0",
    "nuxt": "^3.15.4",
    "pinia": "^2.3.1",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.0.0",
    "@pinia/testing": "^0.1.7",
    "@unocss/nuxt": "^65.4.3",
    "@vitest/coverage-v8": "^3.0.5",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.19.0",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.10"
