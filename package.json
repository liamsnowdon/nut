{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --max-warnings 0",
